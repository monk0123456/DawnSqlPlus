      <groupId>org.apache.thrift</groupId>
      <artifactId>libthrift</artifactId>
      <version>${apache.thrift}</version>
    </dependency>

    <!-- Ignite -->

    <dependency>
      <groupId>org.gridgain</groupId>
      <artifactId>dawn-sql-core</artifactId>
